<h1>結果</h1>
<%  %>
    <% if @judge.post.judge_correct_id == @judge.judge_correct_id %>
    <%= render partial: "shared/correct" %>
    <%else%>
    <%= render partial: "shared/incorrect" %>
    <%end%>

<h1>皆の意見</h1>
    <% @judges.each do |judge|%>
      <%= "#{judge.comment} by#{judge.user.nickname}" %>
    <%end%>

